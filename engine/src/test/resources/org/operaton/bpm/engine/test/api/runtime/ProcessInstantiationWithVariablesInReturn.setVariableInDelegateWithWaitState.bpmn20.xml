<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions" xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:operaton="http://operaton.org/schema/1.0/bpmn" targetNamespace="Examples">

  <process id="variableProcess" isExecutable="true">
    <startEvent id="start" />
    <sequenceFlow id="flow1" sourceRef="start" targetRef="syncTask" />
    <serviceTask id="syncTask" operaton:class="org.operaton.bpm.engine.test.api.runtime.AddVariablesService" />
    <sequenceFlow id="flow2" sourceRef="syncTask" targetRef="user" />
    <userTask id="user"/>
    <sequenceFlow id="flow3" sourceRef="user" targetRef="end" />
    <endEvent id="end" />
  </process>

</definitions>