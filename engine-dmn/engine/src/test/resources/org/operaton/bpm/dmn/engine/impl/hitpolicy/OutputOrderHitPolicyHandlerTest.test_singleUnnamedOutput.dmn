<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/"
             xmlns:modeler="http://camunda.org/schema/modeler/1.0"
             id="Definitions_SortTest"
             name="SortOutputOrder"
             namespace="http://camunda.org/schema/1.0/dmn"
             exporter="Camunda Modeler"
             exporterVersion="5.34.0"
             modeler:executionPlatform="Camunda Platform"
             modeler:executionPlatformVersion="7.23.0">

  <decision id="decision_sort_test" name="SortOutputOrder">
    <decisionTable id="decisionTable_sort" hitPolicy="OUTPUT ORDER">

      <!-- Input -->
      <input id="input1" label="Score">
        <inputExpression id="inputExpression1" typeRef="integer">
          <text>Score</text>
        </inputExpression>
      </input>

      <!-- Output -->
      <output id="output1" typeRef="string">
        <outputValues>
          <text>"Low", "Medium", "High", "Unique"</text>
        </outputValues>
      </output>

      <!-- Rules (intentionally unordered + exclusive rule) -->
      <rule id="rule1">
        <inputEntry><text>&lt; 200</text></inputEntry>
        <outputEntry><text>"High"</text></outputEntry>
      </rule>
      <rule id="rule2">
        <inputEntry><text>&gt;= 600</text></inputEntry>
        <outputEntry><text>"Low"</text></outputEntry>
      </rule>
      <rule id="rule3">
        <inputEntry><text>&gt;= 300</text></inputEntry>
        <outputEntry><text>"Medium"</text></outputEntry>
      </rule>
      <rule id="rule4">
        <inputEntry><text>&gt;= 100</text></inputEntry>
        <outputEntry><text>"Low"</text></outputEntry>
      </rule>
      <rule id="rule5">
        <inputEntry><text>&gt;= 400</text></inputEntry>
        <outputEntry><text>"High"</text></outputEntry>
      </rule>
      <rule id="rule6">
        <inputEntry><text>&lt;= 500</text></inputEntry>
        <outputEntry><text>"Medium"</text></outputEntry>
      </rule>
      <!-- Exclusive rule -->
      <rule id="rule7">
        <inputEntry><text>999</text></inputEntry>
        <outputEntry><text>"Unique"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
