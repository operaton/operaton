<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" 
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="processEngineConfiguration" class="org.operaton.bpm.engine.impl.cfg.StandaloneInMemProcessEngineConfiguration">

    <property name="processEngineName" value="TenantIdProviderTest" />
    <property name="jdbcUrl" value="jdbc:h2:mem:operaton-TenantIdProviderTest" />

    <property name="history" value="${history.level}" />

    <property name="tenantIdProvider" ref="tenantIdProvider"></property>

    <property name="enforceHistoryTimeToLive" value="false" />

  </bean>
  
  <bean id="tenantIdProvider" class="org.operaton.bpm.engine.test.api.multitenancy.TenantIdProviderTest$TestTenantIdProvider"></bean>

</beans>
