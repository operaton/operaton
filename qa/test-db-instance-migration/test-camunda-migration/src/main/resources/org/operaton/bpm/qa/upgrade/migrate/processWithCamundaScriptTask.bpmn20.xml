<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:operaton="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="Examples">
  <process id="processWithCamundaScriptTask" isExecutable="true">
    <startEvent id="theStart" />
    <sequenceFlow id="flow1" sourceRef="theStart" targetRef="userTask" />
    <scriptTask id="scriptTask" scriptFormat="Javascript">
      <script>
        <![CDATA[
          org.camunda.bpm.unpresent.Util.invokeUtil('util');
          org.camunda.bpm.unpresent.Util.invokeUtil('utility');
          org.camunda.bpm.unpresent.Util.invokeUtil('invoke util method');
        ]]>
      </script>
    </scriptTask>
    <sequenceFlow id="flow2" sourceRef="userTask" targetRef="end" />
    <endEvent id="end" />
  </process>
</definitions>
