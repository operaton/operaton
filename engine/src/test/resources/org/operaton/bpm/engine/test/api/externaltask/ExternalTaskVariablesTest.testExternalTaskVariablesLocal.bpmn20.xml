<?xml version="1.0" encoding="UTF-8"?>
<definitions id="taskAssigneeExample"
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:operaton="http://operaton.org/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="oneExternalTaskProcess" isExecutable="true">

    <startEvent id="start"/>

    <sequenceFlow id="flow1" sourceRef="start" targetRef="externalTask" />

    <serviceTask id="externalTask" operaton:type="external" operaton:topic="testTopic">
      <extensionElements>
        <operaton:inputOutput>
          <operaton:inputParameter name="localVar">localVal</operaton:inputParameter>
        </operaton:inputOutput>
      </extensionElements>
    </serviceTask>

    <sequenceFlow id="flow2" sourceRef="externalTask" targetRef="end" />

    <endEvent id="end" />

  </process>

</definitions>